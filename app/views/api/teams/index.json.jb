@teams.map do |team|
  {
    name: team.name,
    pitcher: (team.pitcher),
    catcher: (team.catcher),
    first_base: (team.first_base),
    second_base: (team.second_base),
    third_base: (team.third_base),
    shortstop: (team.shortstop),
    left_field: (team.left_field),
    center_field: (team.center_field),
    right_field: (team.right_field),
  }
end